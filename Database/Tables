CREATE TABLE planetnames (
                             Planet_ID INT NOT NULL primary key,
                             Planet_Node varchar(2) default,
                             Planet_Name varchar(25)
);

CREATE TABLE planetroutes (
                              Planet_routeid int not null primary key,
                              Planet_Source char(5),
                              Planet_Dest char(5),
                              Dest float(2)

)

